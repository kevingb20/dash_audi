    <table id="tblCitas4" class="table table-hover table-striped table-bordered" style="width:100%">
      <thead class="thead-dark">
        <tr>
          <th>id</th>
          <th>Precio</th>
          <th>Descripción</th>
          <th>Kilometraje</th>
          <th>Fecha</th>
