    <input disabled="true" type="text" class="form-control" id="inputValet" placeholder="nombre" >
  </div>
  <div class="col-sm">
    <label for="inputPrecio">Precio:</label>
    <div class="input-group mb-3">
      <div class="input-group-prepend">
        <span class="input-group-text" id="spAddP2">$</span>
      </div>
      <input disabled="true" type="text" class="form-control" id="inputPrecio2" placeholder="0.00" aria-describedby="spAddP2">
    </div>
  </div>
  <div class="col-sm">
    <label for="inputKilometraje2">Kilometraje:</label>
    <div class="input-group mb-3">
      <input disabled="true" type="text" class="form-control" id="inputKilometraje2" placeholder="0000" aria-describedby="spAddkm2">
      <div class="input-group-append">
        <span class="input-group-text" id="spAddkm2">kms</span>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-sm">
    <label for="inputDescripcion2">Descripción del mantenimiento:</label>
    <textarea class="form-control" rows="7" id="inputDescripcion2" placeholder="Seleccione una cita finalizada para mostrar aquí los detalles" disabled></textarea>
  </div>
</div>
