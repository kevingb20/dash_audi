<form onsubmit="AgregarUsuario()">
<div class="row">
  <div class="col-12">

      <div class="form-group">
        <label for="exampleInputNombre">Nombre:</label>
        <input type="text" class="form-control" id="inputNombre" placeholder="Juan" onemptied="LlenarUsuario()" onkeyup="LlenarUsuario()"required>
      </div>
      <div class="form-group">
        <label for="exampleInputApellido">Apellido:</label>
        <input type="text" class="form-control" id="inputApellido" placeholder="Pérez" required>
      </div>

      <div class="form-row">
        <div class="form-group col-md-3">
          <label for="inputState">Tipo</label>
          <select id="inputTipo" class="form-control" onchange="CambiarTipoEmpleado()">
            <option>administrador</option>
            <option>valet</option>
          </select>
        </div>
        <div class="form-group col-md-9">
            <label for="inputUser">Usuario</label>
            <input type="text" class="form-control" aria-describedby="usulHelp" id="inputUsuario" value="admin"required>
            <small id="usuHelp" class="form-text text-muted">Este usuario no podrá ser modificado en un futuro</small>
        </div>
      </div>

      <div class="form-group">
       <label for="exampleInputPassword1">Contraseña</label>
       <input type="password" class="form-control" id="inputPass" placeholder="Contraseña" required>
      <small id="passHelp" class="form-text text-muted">La contraseña debe tener como mínimo 6 caracteres</small>
     </div>
     <div class="form-group">
      <label for="exampleInputPassword1">Confirme la contraseña</label>
      <input type="password" class="form-control" id="inputPass2" placeholder="Contraseña" required>
     <small id="passHelp" class="form-text text-muted">Escriba nuevamente la contraseña para comprobar que sea la misma</small>
    </div>
     <div class="form-group">
       <label for="exampleInputEmail1">Correo</label>
       <input type="email" class="form-control" id="inputCorreo" aria-describedby="emailHelp" placeholder="correo@ejemplo.com" required>
       <small id="emailHelp" class="form-text text-muted">Correo de la empresa</small>
     </div>
     <div class="form-group">
       <label for="exampleInputPhone1">Teléfono:</label>
       <input type="tel" class="form-control" id="inputTelefono" aria-describedby="telHelp" placeholder="0900000000" required>
       <small id="telHelp" class="form-text text-muted" maxlength="10">Teléfono de la empresa</small>
     </div>




  </div>
</div>
<div id="msg_registrarEmpleado" class="alert text-center" role="alert" style="display: none"></div>
<div class="row">
  <div class="mx-auto">
    <button type="submit" class="btn btn-primary" >Registrar</button>

  </div>
</div>
  </form>
